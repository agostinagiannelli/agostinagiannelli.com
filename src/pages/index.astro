---
import Layout from "@/layouts/Layout.astro";
import Container from "@/components/Container.astro";
import Hero from "@/components/Hero.astro";
import Skills from "@/components/Skills.astro";
import Block from "@/components/Block.astro";
import Logos from "@/components/Logos.astro";
---

<Layout
  title="Agostina Giannelli"
  description="Ecommerce | CRO | Digital Project Manager | Product Owner"
  url="https://agostinagiannelli.com"
>
  <Container>
    <Hero />
    <Skills />
    <Logos />
    <Block
      h2="Glad You Stopped By"
      paragraph="Want to know more? Looking forward to hearing from you."
      link="/contact"
      linkText="Get in Touch"
    />
  </Container>
</Layout>
